#MACRO &op &cs len clone default :str_len &sd &string :print " : " &string :print bring len &num :print &newline

func str_len
    &cs n 0
    &cs input clone default
    loop_start: 0 == branch endloop
        &s n 1 + &s input
        goto loop_start
    endloop:
        &sd bring n
endfunc

func dump
    loop_start: :stack_len 0 == branch endloop pop goto loop_start endloop:
endfunc